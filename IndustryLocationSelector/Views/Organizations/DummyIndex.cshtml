@model List<IndustryLocationSelector.Data.Entity.Organizations>

@{
    //Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <div>
        <h4>Organizations</h4>
        <hr />
        @if (Model.Count == 0)
        {
            <p>no records found </p>
        }
        else
        {
            @foreach (var org in Model)
            {



                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayName(org.Name)
                    </dt>
                    <dd>
                        @Html.Display(org.Name)
                    </dd>
                    <dt>
                        @Html.DisplayName(org.OrganizationType.ToString())
                    </dt>
                    <dd>
                        @Html.Display(org.OrganizationType.Name)
                    </dd>
                    <dt>
                        @Html.DisplayName(org.Latitude.ToString())
                    </dt>
                    <dd>
                        @Html.Display(org.Latitude.ToString())
                    </dd>
                    <dt>
                        @Html.DisplayName(org.Longitude.ToString())
                    </dt>
                    <dd>
                        @Html.Display(org.Longitude.ToString())
                    </dd>
                </dl>
                <div>
                    <a asp-action="Edit" asp-route-id="@org.Id">Edit</a> |
                    <a asp-action="Index">Back to List</a>
                </div>
            }

        }

    </div>

</body>
</html>
